{
  "name": "@trendmicro/react-navbar",
  "version": "0.2.4",
  "description": "Trend Micro Components: React Navbar",
  "main": "lib/index.js",
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "prepublish": "npm run lint && npm test && npm run clean && npm run bowersync && npm run build && npm run build-examples",
    "bowersync": "./scripts/bowersync",
    "build": "webpack && npm run cleancss",
    "build-examples": "cd examples; webpack",
    "clean": "rm -f {lib,dist}/*",
    "cleancss": "cleancss -o dist/react-navbar.min.css dist/react-navbar.css",
    "demo": "http-server -p 8000 docs/",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "test": "tap test/*.js --coverage --nyc-arg=--require --nyc-arg=babel-register --nyc-arg=--require --nyc-arg=babel-polyfill",
    "coveralls": "tap test/*.js --coverage --coverage-report=text-lcov --nyc-arg=--require --nyc-arg=babel-register --nyc-arg=--require --nyc-arg=babel-polyfill | coveralls",
    "dev": "cd examples; webpack-dev-server --hot --inline --host 0.0.0.0 --port 8000 --content-base ../docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trendmicro-frontend/react-navbar.git"
  },
  "author": "Cheton Wu <cheton_wu@trend.com.tw>",
  "contributors": [
    {
      "name": "Cheton Wu",
      "email": "cheton_wu@trend.com.tw",
      "url": "https://github.com/cheton"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trendmicro-frontend/react-navbar/issues"
  },
  "homepage": "https://github.com/trendmicro-frontend/react-navbar",
  "keywords": [
    "react",
    "react-navbar",
    "navbar",
    "sticky"
  ],
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0"
  },
  "dependencies": {
    "@trendmicro/react-navs": "^0.8.5",
    "chained-function": "^0.5.0",
    "classnames": "^2.2.5",
    "dom-helpers": "^3.2.1",
    "prop-types": "^15.5.8",
    "uncontrollable": "^4.1.0"
  },
  "devDependencies": {
    "@trendmicro/react-anchor": "~0.5.5",
    "@trendmicro/react-buttons": "~1.0.4",
    "@trendmicro/react-dropdown": "~0.6.11",
    "babel-cli": "~6.24.1",
    "babel-core": "~6.24.1",
    "babel-eslint": "~7.2.3",
    "babel-loader": "~7.0.0",
    "babel-plugin-transform-decorators-legacy": "~1.3.4",
    "babel-preset-es2015": "~6.24.1",
    "babel-preset-react": "~6.24.1",
    "babel-preset-stage-0": "~6.24.1",
    "clean-css": "~4.1.1",
    "clean-css-cli": "~4.0.12",
    "coveralls": "~2.13.1",
    "css-loader": "~0.28.1",
    "eslint": "~3.19.0",
    "eslint-config-trendmicro": "~0.5.1",
    "eslint-loader": "~1.7.1",
    "eslint-plugin-import": "~2.2.0",
    "eslint-plugin-jsx-a11y": "~2.2.3",
    "eslint-plugin-react": "~7.0.0",
    "extract-text-webpack-plugin": "~2.1.0",
    "file-loader": "~0.11.1",
    "find-imports": "~0.5.2",
    "html-webpack-plugin": "~2.28.0",
    "http-server": "~0.10.0",
    "nib": "~1.1.2",
    "react": "^0.14.0 || ^15.0.0",
    "react-dom": "^0.14.0 || ^15.0.0",
    "react-sticky": "~5.0.8",
    "style-loader": "~0.17.0",
    "stylint": "~1.5.9",
    "stylint-loader": "~1.0.0",
    "stylus-loader": "~3.0.1",
    "tap": "~10.3.2",
    "trendmicro-ui": "~0.4.1",
    "url-loader": "~0.5.8",
    "webpack": "~2.5.1",
    "webpack-dev-server": "~2.4.5",
    "which": "~1.2.14"
  }
}
